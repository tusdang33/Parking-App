+            }
+        }
+
+        binding.changeEyeButtonRetype.setOnClickListener { eye ->
+            with(binding.changeEdtRetypePassword) {
+                if (isShowingRetypePassword) {
+                    (eye as ImageView).setImageResource(R.drawable.eye_on_fill)
